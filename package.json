{
  "name": "kompass",
  "version": "0.1.0",
  "private": true,
  "description": "Intelligent Workforce Scheduling & Route Planning Platform",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:optimizer\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:optimizer": "cd optimizer && python -m uvicorn src.main:app --reload --port 8001",
    "build": "npm run build --workspaces",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "test": "npm run test --workspaces --if-present",
    "test:frontend": "npm run test --workspace=frontend",
    "test:backend": "npm run test --workspace=backend",
    "test:optimizer": "cd optimizer && pytest",
    "lint": "npm run lint --workspaces --if-present",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "npm run clean:workspaces && rm -rf node_modules package-lock.json",
    "clean:workspaces": "rm -rf frontend/node_modules frontend/package-lock.json backend/node_modules backend/package-lock.json",
    "db:up": "docker-compose up -d postgres redis",
    "db:down": "docker-compose down",
    "db:migrate": "npm run db:migrate --workspace=backend",
    "db:seed": "npm run db:seed --workspace=backend",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "workforce-scheduling",
    "route-planning",
    "vrp",
    "optimization"
  ],
  "author": "Kompass",
  "license": "UNLICENSED"
}
